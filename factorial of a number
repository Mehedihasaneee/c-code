// Online C compiler to run C program online
#include <stdio.h>
int fact(int a){
        int b ,i;
        int total=1;
        for (i=1;i<=a;i++) {
            total=total*i;
        }
        return total;
}    
int main() {
    
    int n;
    printf("Enter the value: ");
    scanf("%d", &n);
    int result =fact(n);
    printf("fact of %d is : %d ", n,result);
    
    return 0;
}
